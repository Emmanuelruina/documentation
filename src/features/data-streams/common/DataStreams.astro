---
const GettingStarted = await Astro.glob("./gettingStarted.mdx")
const GettingStartedComponent = GettingStarted[0].Content
const StreamsLookupErrorHandler = await Astro.glob("./streamsLookupErrorHandler.mdx")
const StreamsLookupErrorHandlerComponent = StreamsLookupErrorHandler[0].Content

export type Props = {
  section?: "gettingStarted" | "streamsLookupErrorHandler"
}
const { section } = Astro.props as Props
---

{section === "gettingStarted" && <GettingStartedComponent />}
{section === "streamsLookupErrorHandler" && <StreamsLookupErrorHandlerComponent />}
